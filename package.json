{
  "name": "spargojs",
  "version": "0.0.6",
  "description": "The lightweight JavaScript framework for inserting a little reactivity into some markup.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "delete-dist": "del-cli --force ./dist",
    "build": "npm run delete-dist && node build.js",
    "start": "npm run delete-dist && node build.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spargojs/spargo.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Justin Werner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spargojs/spargo/issues"
  },
  "homepage": "https://github.com/spargojs/spargo#readme",
  "devDependencies": {
    "@types/node": "^18.14.2",
    "del-cli": "^5.0.0",
    "npm-dts": "^1.3.12",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "esbuild": "0.17.11",
    "nanoid": "^4.0.1"
  },
  "peerDependencies": {}
}
